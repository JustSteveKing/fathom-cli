{
    "name": "juststeveking/fathom-cli",
    "description": "The Fathom CLI.",
    "keywords": ["fathom", "cli", "juststeveking"],
    "type": "package",
    "homepage": "https://github.com/JustSteveKing/fathom-cli/",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/juststeveking/fathom-cli/issues",
        "source": "https://github.com/juststeveking/fathom-cli/"
    },
    "authors": [
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://www.juststeveking.uk/"
        }
    ],
    "require": {
        "php": "^8.0",
        "juststeveking/laravel-transporter": "^0.7",
        "laravel-zero/framework": "^8.9"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "nunomaduro/larastan": "^0.7.12",
        "nunomaduro/laravel-console-menu": "^3.2",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/fathom"
    ]
}
